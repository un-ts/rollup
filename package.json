{
  "name": "rollup-plugin-insert",
  "version": "0.2.0",
  "description": "string mutation plugin for rollup",
  "main": "index.js",
  "module": "insert.js",
  "jsnext:main": "insert.js",
  "repository": "https://github.com/JounQin/rollup-plugin-insert.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "files": [
    "index.js",
    "insert.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint . --max-warnings 10",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "prepublishOnly": "npm run test && npm run build"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "extends": "1stg"
  },
  "eslintIgnore": [
    "coverage",
    "node_modules",
    "index.js",
    "test",
    "!test/insert.spec.js"
  ],
  "jest": {
    "collectCoverageFrom": [
      "insert.js"
    ],
    "collectCoverage": true
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "peerDependencies": {
    "rollup": "*"
  },
  "dependencies": {
    "rollup-pluginutils": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "eslint": "^6.0.0",
    "eslint-config-1stg": "^3.0.0",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.0",
    "rollup-plugin-buble": "^0.19.2"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  }
}
